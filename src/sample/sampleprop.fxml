<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ControllerProp">
   <top>
      <VBox alignment="CENTER" spacing="10.0" styleClass="gridpane" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <HBox alignment="CENTER" spacing="2.0" styleClass="bar">
               <children>
                  <Label fx:id="lbtit" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="btmin" mnemonicParsing="false" onAction="#minBut" text="_" />
                  <Button fx:id="btcls" mnemonicParsing="false" onAction="#clsBut" text="X" />
               </children>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </HBox>
            <Label text="Les infos de la municipàlité" textAlignment="CENTER">
               <font>
                  <Font name="Berlin Sans FB" size="18.0" />
               </font>
            </Label>
            <Label text="A propos de nous">
               <font>
                  <Font name="Bell MT" size="17.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <bottom>
      <VBox alignment="CENTER" spacing="10.0" styleClass="gridpane" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <Label fx:id="lbl" alignment="CENTER" prefHeight="17.0" prefWidth="198.0" styleClass="msg" />
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </VBox>
   </bottom>
   <center>
      <VBox alignment="CENTER" spacing="10.0" styleClass="gridlane" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="81.0" fitWidth="277.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.png" />
               </image>
            </ImageView>
            <Label fx:id="nom" styleClass="msg" text="Label" />
            <Label fx:id="dt" styleClass="msg" text="Label" />
            <Label fx:id="loc" styleClass="msg" text="Label" />
            <Label fx:id="nbremp" styleClass="msg" text="Label" />
            <Label fx:id="nbrgui" styleClass="msg" text="Label" />
            <Label fx:id="horr" styleClass="msg" text="Label" />
            <Label fx:id="tr" styleClass="msg" text="Label" />
         </children></VBox>
   </center>
</BorderPane>
